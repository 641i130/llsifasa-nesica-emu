stamp_present0.id = 0
stamp_present0.num = 100

stamp_present1.type = 1
stamp_present1.id = 1
stamp_present1.num = 2

stamp_present2.type = 2
stamp_present2.id = 0
stamp_present2.num = 100

stamp_present3.type = 1
stamp_present3.id = 20
stamp_present3.num = 2

stamp_present4.type = 1
stamp_present4.id = 21
stamp_present4.num = 1

stamp_present5.type = 2
stamp_present5.id = 0
stamp_present5.num = 100

stamp_present6.type = 1
stamp_present6.id = 11
stamp_present6.num = 2

stamp_present7.type = 1
stamp_present7.id = 9
stamp_present7.num = 2

stamp_present8.type = 2
stamp_present8.id = 0
stamp_present8.num = 100

stamp_present9.type = 1
stamp_present9.id = 18
stamp_present9.num = 1



