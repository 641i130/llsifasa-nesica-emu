// ---------------------------------------------------------------------------------------------------------------------------------
// ミュージックパネル増量グルコ増量
// ---------------------------------------------------------------------------------------------------------------------------------

/*
, 各モードでのミュージックパネル枚数
, 各モードの初期値〜20で指定可能
*/
mp_num_solo = 1
mp_num_local = 2
mp_num_event = 2

/*
, 新規プレイ時における各モードでのミュージックパネル枚数
, 各モードの初期値〜20で指定可能
*/
// mp_num_new_solo = 10
// mp_num_new_local = 10
// mp_num_new_event = 10


// ---------------------------------------------------------------------------------------------------------------------------------
// グルコ増量
// ---------------------------------------------------------------------------------------------------------------------------------

/*
, 各モードでのグルコ取得倍率
, 1.0〜20.0で指定可能
*/
gm_ratio_solo = 1.0
gm_ratio_local = 1.0
gm_ratio_event = 1.0


// ---------------------------------------------------------------------------------------------------------------------------------
// コラボレーション機能（プレゼント機能）
// ---------------------------------------------------------------------------------------------------------------------------------

/*
, 0..無効　／　1..有効
*/
collabo_enable = 0

/*
, 対象となるゲームID（他のNESiCA対応ゲーム）
, -1を指定すると無条件にプレゼント（collabo_timestamp_startは無視される）
*/
collabo_game_id = -1

/*
, 期間指定（UNIXTIMEで指定する）
, http://konisimple.net/tool/unixtime/?unix=1393552089
, 0を指定すると「一度でも遊んだことがあれば」の条件になる
*/
collabo_timestamp_start = 0

/*
, プレゼントの内容
, それぞれ0〜7まで指定可能
, type : 0..なし
,        1..アバター
,        2..称号
,        3..ナビゲーター(未実装)
, id   : 各ID
*/
collabo_present0.type = 1
collabo_present0.id = 114

// ---------------------------------------------------------------------------------------------------------------------------------
// デイリースタンプ機能
// ---------------------------------------------------------------------------------------------------------------------------------

/*
, 0..無効　／　1..有効
*/
stamp_enable = 1

/*
, 0..このファイルの設定値を使う　／　1..デフォルトの設定値を使う
*/
use_default = 1

/*
, それぞれ0〜9まで指定可能
, type : 0..なし
,        1..アイテム
,        2..グルコ
,        3..ミュージックパネル(廃止)
,        4..アバター(プレゼントBOX)
,        5..称号(プレゼントBOX)
,        6..ナビゲーター(未実装)
, id   : 各ID（アイテム、アバター、称号、ナビゲーターで有効）
, num  : 個数（アイテム、グルコ、アバター/称号のプレゼントBOXで有効）

, ※プレゼントBOXの中身を既に所持していた場合は、numで設定した数のグルコがもらえます。

, ※最初の項目は初回のみ無視されます。
, 　（スタンプカード２週目から取得できる）
*/
